{{> menu id="dashboard"}}

<section class="ui segment" style="border: 3px solid #043F8C; border-radius: 10px; background-color: #C9E5F0;">
  <h2 class="ui header">
    Teams Dashboard: {{user.firstname}} {{user.lastname}}
  </h2>
</section>

<div class="ui two column grid basic segment">
  <div class="column">

  </div>
  {{#if teams}}
  <div class="column">
    
    {{#each teams}}
        {{> teamDashboardSummary}}
    {{/each}}
    
  </div>
  {{/if}}
</div>