<a href="/dashboard/{{_id}}/viewTeam">
<div class="ui segment" style="border: 3px solid #043F8C;">
    <div class="card" id="cardStyle"><h2>{{teamname}}</h3>
    </div>
    <div class="card" id="cardStyle"><h4>Location</h2>
      {{location}}
    </div>
    <div class="card" id="cardStyle"><h4>Fund(s)</h2>
    {{#each funds}}
      <li>{{fundname}}</li>
    {{/each}}
    </div>
    <canvas id="myChart" style="width:100%;max-width:700px"></canvas>
</div>
<br>
</a>

<script>
var xyValues = [
  {x:50, y:7},
  {x:60, y:8},
  {x:70, y:8},
  {x:80, y:9},
  {x:90, y:9},
  {x:100, y:9},
  {x:110, y:10},
  {x:120, y:11},
  {x:130, y:14},
  {x:140, y:14},
  {x:150, y:15}
];

new Chart("myChart", {
  type: "scatter",
  data: {
    datasets: [{
      pointRadius: 4,
      pointBackgroundColor: "rgb(0,0,255)",
      data: xyValues
    }]
  },
  options: {
    legend: {display: false},
    scales: {
      xAxes: [{ticks: {min: 40, max:160}}],
      yAxes: [{ticks: {min: 6, max:16}}],
    }
  }
});
</script>