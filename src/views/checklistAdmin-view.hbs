{{> menu id="checklistAdmin"}}

<section class="ui segment" style="border: 3px solid #043F8C; border-radius: 10px; background-color: #C9E5F0;">
  <h2 class="ui header">
    Add Checklist
  </h2>
</section>

<div class="ui two column grid basic segment">
  <div class="column">
    {{> addChecklist}}
  </div>
  <div class="column">
    {{#each checklists}}
        {{> checklistSummary}}
    {{/each}}
  </div>
</div>